all clean depend: Makefile.xmlread 
	$(MAKE) -f Makefile.xmlread $@

distclean: clean
	$(RM) -f Makefile.xmlread

tmake:
	export TMAKEPATH=$(TMAKEPATH) ; $(PERL) $(TMAKE) xmlread.pro >Makefile.xmlread

Makefile.xmlread: xmlread.pro
	export TMAKEPATH=$(TMAKEPATH) ; $(PERL) $(TMAKE) xmlread.pro >Makefile.xmlread

